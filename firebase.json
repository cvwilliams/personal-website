{
    "hosting": {
        "public": "public",
        "ignore": [
            "firebase.json",
            "**/.*",
            "**/node_modules/**"
        ],
        "headers": [{
            "source" : "**/*.@(jpg|jpeg|gif|png|svg|js|css)",
            "headers" : [{
              "key" : "Cache-Control",
              "value" : "max-age=7200"
            }]
        }]
    }
}